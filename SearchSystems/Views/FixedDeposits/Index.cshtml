@model IEnumerable<SearchSystems.Models.FixedDeposit>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Account Name")
        </th>
        <th>
            @Html.DisplayName("FD Number")
        </th>
        <th>
            @Html.DisplayName("Purchase Date")
        </th>
        <th>
            @Html.DisplayName("Amount")
        </th>
        <th>
            @Html.DisplayName("Rate of Interest")
        </th>
        <th>
            @Html.DisplayName("Maturity Date")
        </th>
        <th>
            @Html.DisplayName("Maturity Amount")
        </th>
        <th>
            @Html.DisplayName("Status")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AccountName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FDNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateOfPurchase)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Amount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RateOfInterest)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MaturityDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MaturityAmount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { serialnumber = item.SerialNumber, bankname = item.BankName }) |
            @Html.ActionLink("Details", "Details", new { serialnumber = item.SerialNumber, bankname = item.BankName }) |
            @Html.ActionLink("Delete", "Delete", new { serialnumber = item.SerialNumber, bankname = item.BankName })
        </td>
    </tr>
}

</table>
