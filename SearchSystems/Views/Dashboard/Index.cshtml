                <tr style="color:lightseagreen">
                    <th>Blood Group</th>
                    <th>Total Employees</th>
                </tr>
                @foreach (var item in Model.BloodGroups)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Key)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Value)
                        </td>
                    </tr>

                }

            </table>
        </div>
        <div class="col-md-4">
            <h4 style="color:lightseagreen">@Html.Label("SalaryWise Distribution Active")</h4>
            <table border="1" class="text-center" cellpadding="5">
                <tr style="color:lightseagreen">
                    <th>     Salary Range     </th>
                    <th>Total Male Employees</th>
                    <th>Total Female Employees</th>
                </tr>

                @foreach (var item in Model.SalaryRangeMaleFemaleDistributions)
                {
                    <tr>
                        <td nowrap="">
                            @Html.DisplayFor(modelItem => item.SalaryRange)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TotalMaleEmployees)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TotalFemaleEmployees)
                        </td>
                    </tr>

                }

            </table>
        </div>
        <div class="col-md-4">
            <h4 style="color:lightseagreen">@Html.Label("Upcoming Birthdays Active")</h4>
            <table border="1" class="text-center" cellpadding="5">
                @if (Model.EmployeeBirthdays.Count() > 0)
                {
                    <tr style="color:lightseagreen">
                        <th>Name</th>
                        <th>Birthday</th>
                    </tr>

                    foreach (var item in Model.EmployeeBirthdays)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Key)
                            </td>
                            <td>
                                @Html.TextBox("birthday", item.Value.Value.ToShortDateString(), new { @readonly = "readonly", @style = "border: 0px" })
                            </td>
                        </tr>

                    }
                }
                else
                { @Html.Label("No Birthdays in the coming month!")

            }

            </table>
        </div>
    </div>


